{
  "rules": {
    "brace-style": [2, "1tbs"],
    "comma-style": [2, "last"],
    "indent": [2, 2, {"SwitchCase": 1}],
    "no-delete-var": 2,
    "no-lonely-if": 2,
    "no-mixed-spaces-and-tabs": [2, "smart-tabs"],
    "no-multi-spaces": 2,
    "no-unused-vars": 2,
    "radix": 2,
    "react/jsx-uses-vars": 1,
    "react/jsx-uses-react": 1,
    "semi": [2, "never"],
    "space-after-keywords": 2,
    "space-before-blocks": 2,
    "space-before-function-paren": 2,
    "space-before-keywords": [2, "always"],
    "space-in-parens": [2, "never"],
    "space-infix-ops": 2,
    "space-return-throw-case": 2,
    "space-unary-ops": 2,
    "yoda": 2
  },
  "ecmaFeatures": {
    "arrowFunctions": true,
    "classes": true,
    "defaultParams": true,
    "destructuring": true,
    "restParams": true,
    "spread": true,
    "templateStrings": true,
    "jsx": true,
    "experimentalObjectRestSpread": true,
    "modules": true
  },
  "env": {
    "es6": true
  },
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ]
}
